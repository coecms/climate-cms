<?xml version="1.0" encoding="UTF-8"?>
<catalog name="THREDDS Server Default Catalog : You must change this to fit your server!"
    xmlns="http://www.unidata.ucar.edu/namespaces/thredds/InvCatalog/v1.0"
    xmlns:xlink="http://www.w3.org/1999/xlink">

    <service name="all" base="" serviceType="compound">
        <service name="odap" serviceType="OpenDAP" base="/thredds/dodsC/" />
        <service name="http" serviceType="HTTPServer" base="/thredds/fileServer/" />
        <service name="wcs" serviceType="WCS" base="/thredds/wcs/" />
        <service name="wms" serviceType="WMS" base="/thredds/wms/" />
        <service name="ncml" serviceType="NCML" base="/thredds/ncml/" />
        <service name="uddc" serviceType="UDDC" base="/thredds/uddc/" />
        <service name="iso" serviceType="ISO" base="/thredds/iso/" />
        <service name="ncss" serviceType="NetcdfSubset" base="/thredds/ncss/grid/" />
    </service>

    <datasetScan name="arccss-data" ID="arccssdata" path="ARCCSS_Data" location="/g/data1/ua8/ARCCSS_Data/" restrictAccess="ua8">
        <metadata inherited="true">
            <serviceName>all</serviceName>
            <authority>ARCCSS</authority>
            <dataType>GRID</dataType>
        </metadata>
        <filter>
            <include wildcard="*.nc" />
        </filter>
    </datasetScan>

    <datasetScan name="ocean-data" ID="oceandata" path="Ocean_Data" location="/g/data1/v45/pas561/mom/core_nyf/gfdl_1080/">
        <metadata inherited="true">
            <serviceName>all</serviceName>
            <authority>ARCCSS-ocean</authority>
            <dataType>GRID</dataType>
        </metadata>
        <filter>
            <include wildcard="*.nc" />
            <exclude wildcard="restart*" atomic="false" collection="true"/>
        </filter>
    </datasetScan>

    <datasetScan name="Public Ocean Datasets" ID="v45-public" path="v45-public" location="/g/data1/v45/thredds/public/">
        <metadata inherited="true">
            <serviceName>all</serviceName>
            <authority>ARCCSS-ocean</authority>
            <dataType>GRID</dataType>
        </metadata>
        <filter>
            <include wildcard="*.nc" />
            <exclude wildcard="restart*" atomic="false" collection="true"/>
        </filter>
    </datasetScan>

    <datasetScan name="Internal Ocean Datasets" ID="v45-private" path="v45-private" location="/g/data1/v45/thredds/private/" restrictAccess="v45">
        <metadata inherited="true">
            <serviceName>all</serviceName>
            <authority>ARCCSS-ocean</authority>
            <dataType>GRID</dataType>
        </metadata>
        <filter>
            <include wildcard="*.nc" />
            <exclude wildcard="restart*" atomic="false" collection="true"/>
        </filter>
    </datasetScan>

    <datasetScan name="LAS" path="las" location="/usr/share/tomcat/content/las/conf/server/data" serviceName="all">
        <filter>
            <include wildcard="*.nc"/>
            <include wildcard="*.fds"/>
            <include wildcard="*.jnl"/>
        </filter>
    </datasetScan>

    <catalogRef xlink:title="Test Enhanced Catalog" xlink:href="enhancedCatalog.xml" name=""/>
    <catalogRef xlink:title="Example time aggregation" xlink:href="time-aggregation.xml" name=""/>
    <catalogRef xlink:title="Example variables aggregation" xlink:href="var-aggregation.xml" name=""/>

</catalog>
